[run]
source =
    reversion
    test_app
    test_project

[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError
    assert False

    # Don't complain if tests don't hit model __str__ methods.
    def __str__

show_missing = True
skip_covered = True
